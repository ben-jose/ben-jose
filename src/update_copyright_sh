
# Only change last year. DO NOT use generic matching '^Copyright' because
# it will change other people's Copyright.

find . -name "*" -type f | xargs sed -i 's/^Copyright (C) 2007-2012, 2014-2016/Copyright (C) 2007-2012, 2014-2016/'

sed -i 's/^Copyright (C) 2007-2012, 2014-2016/Copyright (C) 2007-2012, 2014-2016/' ../README.txt

